    "format": "prettier --semi false --write *.js *.d.ts"
  },
  "devDependencies": {
    "prettier": "~1.4.2",
    "rollup": "^0.42.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "typings": "picodom.d.ts"
}
