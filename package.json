  "version": "0.0.7",
  "main": "dist/picodom.js",
  "jsnext:main": "index.js",
  "module": "index.js",
  "license": "MIT",
  "repository": "picodom/picodom",
  "files": [
    "index.js",
    "dist"
  ],
  "author": "Jorge Bucaran",
  "keywords": [
    "hyperscript",
    "virtual-dom"
  ],
  "scripts": {
    "test": "exit 0",
