declare module "picodom" {
  declare type VirtualNode = {
    tag: string,
    data: { [string]: any },
    children: Array<VirtualNode | string>
  };

  declare type VirtualComponent = (
    data: { [string]: any } | null,
    children: Array<VirtualNode | string> | VirtualNode | string
  ) => VirtualNode;

  declare export function h(
    tag: VirtualComponent,
    data?: any
  ): VirtualNode;

  declare export function patch(
    oldNode: VirtualNode | null,
    node: VirtualNode,
    element: Node | Text | Element| HTMLElement | null,
    parent: Node | Text | Element | HTMLElement | null,
    cb: Function
  ): Node | Text | Element | HTMLElement;
}